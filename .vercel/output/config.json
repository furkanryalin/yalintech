{
	"version": 3,
	"routes": [
		{
			"handle": "filesystem"
		},
		{
			"src": "^/_astro/(.*)$",
			"headers": {
				"cache-control": "public, max-age=31536000, immutable"
			},
			"continue": true
		},
		{
			"src": "^/_server-islands/([^/]+?)/?$",
			"dest": "_render"
		},
		{
			"src": "^/_image/?$",
			"dest": "_render"
		},
		{
			"src": "^/api/admin/comments/([^/]+?)/?$",
			"dest": "_render"
		},
		{
			"src": "^/api/admin/comments/?$",
			"dest": "_render"
		},
		{
			"src": "^/api/admin/login/?$",
			"dest": "_render"
		},
		{
			"src": "^/api/admin/status/?$",
			"dest": "_render"
		},
		{
			"src": "^/api/admin/users/([^/]+?)/?$",
			"dest": "_render"
		},
		{
			"src": "^/api/admin/users/?$",
			"dest": "_render"
		},
		{
			"src": "^/api/auth/check/?$",
			"dest": "_render"
		},
		{
			"src": "^/api/auth/register/?$",
			"dest": "_render"
		},
		{
			"src": "^/api/auth/resend-request/?$",
			"dest": "_render"
		},
		{
			"src": "^/api/comments/?$",
			"dest": "_render"
		},
		{
			"src": "^/api/contact/?$",
			"dest": "_render"
		},
		{
			"src": "^/api/messages/([^/]+?)/?$",
			"dest": "_render"
		},
		{
			"src": "^/api/messages/?$",
			"dest": "_render"
		},
		{
			"src": "^/api/register-profile/?$",
			"dest": "_render"
		},
		{
			"src": "^/.*$",
			"dest": "/404.html",
			"status": 404
		}
	]
}